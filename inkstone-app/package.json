{
  "name": "inkstone-app",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/user-event": "14.1.1",
    "concurrently": "7.1.0",
    "firebase": "9.7",
    "i18next": "21.6.16",
    "i18next-browser-languagedetector": "6.1.4",
    "i18next-http-backend": "1.4.0",
    "node-sass": "7.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-i18next": "11.16.7",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "reactjs-popup": "2.0.5",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watchSass": "node-sass -w src -o public/css",
    "sass": "node-sass src -o public/css",
    "watchType": "tsc -w",
    "watch": "concurrently --kill-others \"npm run watchSass\" \"npm run watchType\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "_",
          "varsIgnorePattern": "_"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "27.5.0",
    "@types/node": "17.0.31",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.3",
    "@types/react-router-dom": "5.3.3",
    "typescript": "4.6.4"
  }
}
